# Audio
audio.offload.min.duration.secs=30
persist.vendor.audio.hac.enable=false
persist.vendor.audio_hal.dsp_bit_width_enforce_mode=24

# Camera
camera.disable_zsl_mode=true

# Charger
ro.charger.disable_init_blank=true

# Display
debug.gralloc.gfx_ubwc_disable=0
debug.sf.enable_hwc_vds=1
debug.sf.latch_unsignaled=1
persist.vendor.color.matrix=2

# CNE
persist.vendor.cne.feature=1

# DPM
persist.vendor.dpm.feature=9

# IMS
persist.dbg.volte_avail_ovr=1
persist.dbg.vt_avail_ovr=1
persist.dbg.wfc_avail_ovr=1

# IOP
vendor.iop.enable_prefetch_ofr=1
vendor.iop.enable_uxe=0

# Media
media.settings.xml=/system/etc/media_profiles_vendor.xml

# Perf
ro.vendor.qti.core_ctl_max_cpu=4
ro.vendor.qti.core_ctl_min_cpu=2

# Pre-Rendering
ro.vendor.perf.scroll_opt=true

# QTI Bluetooth
#ro.bluetooth.library_name=libbluetooth_qti.so
vendor.bluetooth.soc=cherokee
persist.vendor.btstack.enable.splita2dp=true
persist.vendor.btstack.a2dp_offload_cap=sbc-aptx-aptxtws-aptxhd-aac-ldac
persist.vendor.btstack.enable.twsplus=true
persist.vendor.bt.a2dp.aac_whitelist=false

# Radio
ro.telephony.default_network=33,33
persist.sys.fflag.override.settings_network_and_internet_v2=true
persist.vendor.radio.mt_sms_ack=30
persist.sys.fflag.override.settings_provider_model=false

# Touchscreen
persist.vendor.qti.inputopts.enable=true
persist.vendor.qti.inputopts.movetouchslop=0.6

# Netmgr
persist.vendor.data.iwlan.enable=true
persist.vendor.data.mode=concurrent

# SSR
persist.vendor.ssr.restart_level=ALL_ENABLE

# SOMC
ro.semc.ms_type_id=PM-1221-BV
ro.semc.product.device=J9110
ro.semc.product.model=J9110
ro.semc.product.name=Xperia 1
ro.semc.version.fs=GENERIC
ro.semc.version.fs_revision=55.2.A.4.154
ro.semc.version.sw=1318-2688
ro.semc.version.sw_revision=55.2.A.4.154
ro.semc.version.sw_type=user
ro.semc.version.sw_variant=CHINA-A2

# Vendor
ro.vendor.qti.va_aosp.support=1

# Zygote
persist.device_config.runtime_native.usap_pool_enabled=true

ro.hwui.use_vulkan=true
ro.hardware.vulkan=adreno
graphics.gpu.profiler.support=true
renderthread.skia.reduceopstasksplitting=true
debug.hwui.renderer=skiavk

# Dolby atmos for Sound Enhancement
persist.sony.effect.dolby_atmos=true
# Gamut
ro.sys.colorgamut.supported=true
persist.sys.colorgamut.mode=1

# Added system property for RID009076
persist.service.automaster.mode=true
persist.sys.colorgamut.current.mode=1

# Touch
sys.preactivated.smart_on_display=1

# X-Reality for mobile
ro.service.xrfm.supported=true
persist.service.xrfm.mode=1

# 4K for Youtube
sys.display-size=3840x1644

# Sensors
persist.vendor.sensors.enable.mag_filter=true

debug.force_no_blanking=true
